<section>

  <div id="likeModal" class="modal">
  <p>Added to Dashboard!</p>
    </div>
    <h1>Welcome to the Quotes Collection</h1>
    
    <ul>
        {{log logged_in}}
        {{#each quotes}}
        <li class="caveat-medium">{{this.quote}} - <strong>{{this.author}}</strong></li>
        {{log this.id}}
        {{#if ../logged_in}}
        <button class="add-quote-btn favorite-btn" data-quote-id="{{this.id}}">Like</button>
        {{log this.id}}
        {{/if}}
        {{/each}}
    </ul>

</section>  